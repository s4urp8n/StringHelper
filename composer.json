{
  "name": "zver/string-helper",
  "description": "this is universal package template for development and testing",
  "minimum-stability": "stable",
  "config": {
    "optimize-autoloader": true
  },
  "autoload": {
    "psr-4": {
      "": "src/"
    },
    "files": [
      "src/Functions.php"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "": "tests/"
    }
  },
  "require": {
    "zver/common": "^3.0",
    "ext-mbstring": "*",
    "ext-json": "*",
    "ext-intl": "*"
  },
  "require-dev": {
    "phpunit/phpunit": "^8.5"
  }
}
